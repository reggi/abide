{
  "name": "@reggi/replace-cli",
  "version": "1.0.0",
  "author": "Thomas Reggi",
  "main": "./index.build.js",
  "scripts": {
    "babel": "babel ./index.js --out-file ./index.build.js",
    "babel:watch": "npm run babel -- --watch",
    "standard": "standard",
    "test": "jest --coverage",
    "test:result": "npm run test --silent &>/dev/null || echo $?"
  },
  "babel": {
    "env": {
      "plugins": [
        "istanbul",
        "rewire",
        "transform-define"
      ]
    },
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ]
  },
  "jest": {
    "collectCoverage": true
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "decache": "^4.4.0",
    "event-as-promised": "^1.0.0",
    "execa": "^0.10.0",
    "hook-exit": "^1.0.1",
    "jest": "^22.4.2",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1",
    "sandboxed-module": "^2.0.3",
    "sinon": "^4.4.8",
    "standard": "^11.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "index.build.js"
    ],
    "globals": [
      "expect",
      "test",
      "beforeEach",
      "afterEach"
    ]
  },
  "dependencies": {
    "@reggi/cli-test": "file:../cli-test",
    "@reggi/process": "file:../process",
    "@reggi/stdio": "file:../stdio",
    "bdd-stdin": "^0.2.0",
    "get-stdin": "^6.0.0",
    "intercept-stdout": "^0.1.2",
    "mock-cli": "^0.1.2",
    "mock-stdin": "^0.3.1",
    "natives": "^1.1.2",
    "stdio-mock": "^1.1.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "html"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "babel": {
      "presets": [
        "env"
      ],
      "env": {
        "test": {
          "plugins": [
            "istanbul"
          ]
        }
      }
    }
  }
}
